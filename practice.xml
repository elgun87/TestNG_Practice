<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="tests" thread-count="2">
    <groups>
        <run>
            <exclude name="smoke"/>
        </run>
    </groups>
    <test verbose="2" preserve-order="true" name="Car Loan">
        <classes>
            <class name="Practice.package_1.CarLoan"/>
        </classes>
    </test>
    <test name="Mobile Loan">
        <classes>
            <class name="Practice.package_1.MobileLoan"/>
        </classes>
    </test>
    <test name="Phone Loan">
        <classes>
            <class name="Practice.package_1.PhoneLoan"/>
        </classes>
    </test>
</suite>